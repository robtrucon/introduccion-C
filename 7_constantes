#include <stdio.h>

// Definir una constante llamada PI
#define PI 3.14159265359

int main() {
    // Calcular el área de un círculo utilizando la constante PI
    double radio = 5.0;
    double area = PI * radio * radio;

    // Imprimir el resultado
    printf("El área del círculo es: %f\n", area);

    return 0;
}
